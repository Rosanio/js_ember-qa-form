<h1>?</h1>

<div class="row">
  <div class="col-xs-6">
    <h3>Ask a Question</h3>
    {{new-question askQuestion='askQuestion'}}
  </div>
  <div class="col-xs-6">
    <h3>Answer Questions</h3>
    {{#each model as |question|}}
      <div>
        {{#link-to 'question' question.id}}{{question.content}}{{/link-to}}
      </div>
    {{/each}}
  </div>
</div>
